{
  "zuix": {
    "build": {
      "input": "source",
      "output": "docs",
      "copy": [
        "css",
        "images",
        ".nojekyll",
        "browserconfig.xml",
        "manifest.json",
        "humans.txt",
        "favicon.ico",
        "robots.txt",
        "content/**/*.jpg",
        "content/**/*.png",
        "content/**/*.webp",
        "content/**/*.gif",
        "content/**/*.svg",
        "content/**/*.json"
      ],
      "ignore": [
        "_filters",
        "content/**/_inc/*"
      ],
      "dataFolder": "_data",
      "includesFolder": "_inc",
      "componentsFolders": [ "app", "lib" ],
      "contentFolder": "content",
      "dependencies": {
        "zuix-dist/js": "js/zuix",
        "elasticlunr/release": "js/elasticlunr",
        "flex-layout-attribute/css": "css/fla"
      },
      "bundle": {
        "js": true,
        "css": true,
        "zuix": true
      },
      "mirror": "",
      "serviceWorker": true,
      "minify": {
        "disable": false,
        "collapseWhitespace": true,
        "removeOptionalTags": true,
        "removeRedundantAttributes": true,
        "removeScriptTypeAttributes": true,
        "removeTagWhitespace": false,
        "useShortDoctype": false,
        "collapseBooleanAttributes": true,
        "removeAttributeQuotes": false,
        "removeEmptyAttributes": true,
        "minifyCSS": false,
        "minifyJS": true
      }
    },
    "app": {
      "title": "Web Starter",
      "subtitle": "Simple, yet powerful!",
      "baseUrl": "/zuix-web-starter/",
      "resourcePath": "{{ app.baseUrl }}app/",
      "libraryPath": {
        "@lib": "{{ app.baseUrl }}lib/1.1/",
        "@hgui": "https://genielabs.github.io/homegenie-web-ui/app/",
        "@cdnjs": "https://cdnjs.cloudflare.com/ajax/libs/"
      },
      "siteMapUrl": "https://zuixjs.github.io{{ app.baseUrl }}"
    }
  }
}
